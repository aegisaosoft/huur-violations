{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Database": {
    "Host": "huur-rental-database.postgres.database.azure.com",
    "Port": 5432,
    "Database": "huur_car_rentals",
    "Username": "alex",
    "Password": "Kis@1963",
    "Pooling": true,
    "MinPoolSize": 5,
    "MaxPoolSize": 200,
    "ConnectionLifetime": 0,
    "SSLMode": "Require"
  },
  "DatabaseSettings": {
    "Provider": "PostgreSQL",
    "CommandTimeout": 60,
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "EnableRetryOnFailure": true,
    "MaxRetryCount": 5,
    "MaxRetryDelay": "00:01:00",
    "EnableServiceProviderCaching": true,
    "EnableQuerySplittingBehavior": "SplitQuery"
  },
  "JwtSettings": {
    "SecretKey": "production-super-secret-jwt-key-that-should-be-at-least-64-characters-long-for-maximum-security",
    "Issuer": "CarRentalAPI-Prod",
    "Audience": "CarRentalClients-Prod",
    "ExpiryMinutes": 30
  }
}
