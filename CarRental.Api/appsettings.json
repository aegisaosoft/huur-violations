{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Host": "huur-rental-database.postgres.database.azure.com",
    "Port": 5432,
    "Database": "huur_car_rentals",
    "Username": "alex",
    "Password": "Kis@1963",
    "Pooling": true,
    "MinPoolSize": 0,
    "MaxPoolSize": 100,
    "ConnectionLifetime": 0,
    "SSLMode": "Require"
  },
  "DatabaseSettings": {
    "Provider": "PostgreSQL",
    "CommandTimeout": 30,
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "EnableRetryOnFailure": true,
    "MaxRetryCount": 3,
    "MaxRetryDelay": "00:00:30",
    "EnableServiceProviderCaching": true,
    "EnableQuerySplittingBehavior": "SplitQuery"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-jwt-key-that-should-be-at-least-32-characters-long-for-production-use",
    "Issuer": "CarRentalAPI",
    "Audience": "CarRentalClients",
    "ExpiryMinutes": 60
  }
}